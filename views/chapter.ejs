<!DOCTYPE html>
<html lang="en">

<head>
  <link href="./styles/chapter.css" rel="stylesheet">
  <link href="./styles/main.css" rel="stylesheet">
  <script src="./sketch.js"></script>
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.7.0/jquery.min.js"></script>
  <title><%= title %></title>

  <script src="https://polyfill.io/v3/polyfill.min.js?features=es6"></script>
  <script id="MathJax-script" async src="https://cdn.jsdelivr.net/npm/mathjax@3.0.1/es5/tex-mml-chtml.js"></script>
</head>

<body style="margin-left: 3%;">
  <%- include('./partials/header.ejs') %>

  <%- chapterHtml %>
  <script>
    let chapterSection = <%= currentSection %>;
    let currentChapter = <%= currentChapter %>;
    let id = <%= id %>;
    let course = new Course(chapterSection, currentChapter, id);
    course.showSections(<%= chapterNumber %>);
  </script>
</body>

</html>